package game;

import jplay.Sprite;
import jplay.URL;

public class Jogador extends Sprite{

	public Jogador(int x, int y) {
		super(URL.sprite("1651116_orig2.png"), 16);
		this.x = x;
		this.y = y;
		System.out.println(x+ " ,"+ y);
		
		// TODO Auto-generated constructor stub
	}
	public void mover(){
		moveX(0.3);
		moveY(0.3);
	}

}
