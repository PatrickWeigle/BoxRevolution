package game;
import jplay.*;
public class Login {
	private Window janela;
	private Scene cena;
	
	public Login(Window window){
		janela = window;
		cena = new Scene();
		cena.loadFromFile(URL.scenario("login.scn"));		
		
		run();
	}
	private void run() {
		// TODO Auto-generated method stub
		while(true){
			cena.draw();			
			janela.update();
		}
		
	}

}
